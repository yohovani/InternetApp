<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".home">
	<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent" tools:layout_editor_absoluteY="1dp"
			tools:layout_editor_absoluteX="1dp">
		<Button
				android:text="Listar Clientes"
				android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/btnClientes"/>
		<Button
				android:text="Mostar Mapa"
				android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/btmMapa"/>
		<Button
				android:text="Cobros"
				android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/btnConbros"/>
	</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>